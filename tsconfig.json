{
    "compileOnSave": true,
    "compilerOptions": {
        "outFile": "dist/Life.js",
        "sourceMap": true,
        "importHelpers": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitOnError": true,
        "removeComments": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "module": "System",
        "target": "ESNext"
    },
    "include": [
        "src/**/*"
    ]
}
